                                </p:row>
                                <p:row>
                                    <p:column style="#{consultasExpedienteMB.personaIdentificacion.tipoPersona == 'M' ? 'display:none' : 'display:disabled;'}">
                                        <p:outputLabel value="#{msgValidacion.ce2ApellidoPaterno}"/>
