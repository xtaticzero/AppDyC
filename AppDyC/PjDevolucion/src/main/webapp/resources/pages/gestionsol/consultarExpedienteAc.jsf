                                         rowsPerPageTemplate="5,10,15,20" selectionMode="single"
                                         selection="#{consultasExpedienteMB.dyctArchivoDTO}"
                                         paginatorTemplate="{CurrentPageReport}  {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}">
                                <p:ajax event="rowSelect" update="descDocumento"
                                        listener="#{consultasExpedienteMB.onRowSelectDocs}"/>
                                <f:facet name="header">
                                    <div align="left">
                                        <p:commandButton id="descDocumento" icon="ui-icon-disk"
                                                         title="Descargar Documento Adjunto" ajax="false"
                                                         immediate="true"
                                                         disabled="#{consultasExpedienteMB.varBotonDocs}"
                                                         actionListener="#{consultasExpedienteMB.downloadDocs}">
                                            <p:fileDownload value="#{consultasExpedienteMB.fileDocs}"/>
                                        </p:commandButton>
                                         
                                        <!--p:commandButton id="loadDocumento" icon="ui-icon-document"
                                                         title="Cargar Documento Adjunto" oncomplete="dlgCargar.show();"
                                                         actionListener="#{consultasExpedienteMB.clear}"
                                                         update="tblAdjuntos"/-->
                                    </div>
                                </f:facet>
                                <p:column headerText="#{msgValidacion.ce2FechaRegistro}">
                                    <h:outputLabel value="#{doc.fechaRegistro}">
                                        <f:convertDateTime pattern="dd/MM/yyyy"/>
                                    </h:outputLabel>
                                </p:column>
                                <p:column headerText="#{msgValidacion.ce2NomDelDocu}">
                                    <h:outputLabel value="#{doc.nombreArchivo}"/>
                                </p:column>
                                <p:column headerText="#{msgValidacion.ce2Estado}">
                                    <h:outputLabel value="Guardado"/>
                                </p:column>
                            </p:dataTable>
                        </p:fieldset>
                    </p:tab>
                    <!--DETALLE CONTRIBUYENTE-->
                    <p:tab id="tab3" title="#{msgValidacion.ce2DetalleContribuyente}">
                        <!--DATOS GENERALES DEL CONTRIBUYENTE-->
                        <p:fieldset legend="#{msgValidacion.ce2DatosGeneralesContribuyente}" style="width:790px">
